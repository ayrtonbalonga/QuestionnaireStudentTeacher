<Window x:Class="Questionnaire.Student"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Questionnaire"
        mc:Ignorable="d"
        Title="Student" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnResults" Content="Results" HorizontalAlignment="Left" Margin="260,60,0,0" VerticalAlignment="Top" Width="128" RenderTransformOrigin="-4.4,-3.75" Click="BtnResults_Click" Height="240"/>
        <Button x:Name="btnDoTest" Content="Do Test" HorizontalAlignment="Left" Margin="38,60,0,0" VerticalAlignment="Top" Width="127" RenderTransformOrigin="-4.4,-3.75" Click="BtnDoTest_Click" Height="240"/>
        <Button x:Name="btnLogOut" Content="Log Out" HorizontalAlignment="Left" Margin="680,355,0,0" VerticalAlignment="Top" Width="75" Click="BtnLogOut_Click" RenderTransformOrigin="-0.267,0.5"/>
    </Grid>
</Window>
