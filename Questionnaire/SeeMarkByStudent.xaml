        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Questionnaire"
        mc:Ignorable="d"
        Title="SeeMarkByStudent" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" Margin="45,360,0,0" VerticalAlignment="Top" Width="75" Click="BtnBack_Click"/>
        <RichTextBox x:Name="rtbDisplay" HorizontalAlignment="Left" Height="195" Margin="85,95,0,0" VerticalAlignment="Top" Width="630">
            <FlowDocument>
                <Paragraph>
                    <Run Text=""/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="btnDisplay" Content="Display" HorizontalAlignment="Left" Margin="325,35,0,0" VerticalAlignment="Top" Width="75" Click="BtnDisplay_Click"/>

    </Grid>
</Window>
